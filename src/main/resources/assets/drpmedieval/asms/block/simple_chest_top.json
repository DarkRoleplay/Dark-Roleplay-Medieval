{
	"parameters": {
		"end_anim": [ "compose", [ "+", 0.48 ] , "#click_time" ],
		"trigger_anim": [ "-", "#end_anim"],
		"progress": [ "compose", [ "*", 2 ] , ["-", "#click_time"]]
	},
	"clips": {
		"closed": "drpmedieval:block/wood_blocks/simple_chest_top@closed",
		"opening": ["trigger_positive", [ "apply", "drpmedieval:block/wood_blocks/simple_chest_top@opening", "#progress" ], "#trigger_anim", "!transition:open" ],
		"open": "drpmedieval:block/wood_blocks/simple_chest_top@open",
		"closing": ["trigger_positive", [ "apply", "drpmedieval:block/wood_blocks/simple_chest_top@closing", "#progress" ], "#trigger_anim", "!transition:closed" ]
	},
	"states": [
		"closed",
		"opening",
		"open",
		"closing"
	],
	"transitions": {
		"closed": ["opening", "open", "closing"],
		"opening": ["open", "closing", "closed"],
		"open": ["closing", "closed", "opening"],
		"closing": ["closed", "opening", "open"]
	},
	"start_state": "closed"
}