{
	"parameters": {
		"end_anim": [ "compose", [ "+", 1 ] , "#click_time" ],
		"trigger_anim": [ "-", "#end_anim" ],
		"progress": [ "-", "#click_time" ]
	},
	"clips": {
		"model": "drpmedieval:block/simple_chest@moving",
		"closed": [ "apply", "#model", 0.0 ],
		"open": [ "apply", "#model", 1.0 ],
		"opening": [
			"trigger_positive",
			[ "slerp", "#closed", "#open", "#identity", "#progress" ],
			"#trigger_anim",
			"!transition:open"
		],
		"closing": [
			"trigger_positive",
			[ "slerp", "#open", "#closed", "#identity", "#progress" ],
			"#trigger_anim",
			"!transition:closed"
		]
	},
	"states": [
		"closed",
		"opening",
		"open",
		"closing"
	],
	"transitions": {
		"closed": "opening",
		"opening": "open",
		"open": "closing",
		"closing": "closed"
	},
	"start_state": "closed"
}